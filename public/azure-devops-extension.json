{
  //"baseUri": "",
  "manifestVersion": 1,
  "id": "EdTro-AzureDevOps-Extensions-QueryBasedBoards",
  "publisher": "EdTro",
  "version": "0.1.0",
  "name": "Query Based Boards",
  "description": "Query Based Boards enables a user to visualize the result of work item queries as a board and track the dependencies",
  "branding": {
    "color": "rgb(217, 217, 217)",
    "theme": "light"
  },
  //"branding": {
  //  "color": "rgb(0, 85, 115)",
  //  "theme": "dark"
  //},
  "categories": [
    "Azure Boards"
  ],
  "demands": [
    "api-version/5.0"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Board",
    "Boards",
    "Query",
    "Queries",
    "WIQL",
    "Dependency",
    "Dependencies"
  ],
  "scopes": [
    "vso.extension.data",
    "vso.extension.data_write",
    "vso.work",
    "vso.work_write"
  ],
  "icons": {
    "default": "img/logo.png"
  },
  "content": {
    "details": {
      "path": "details.md"
    }
  },
  "files": [
    {
      "path": "img",
      "addressable": true,
      "packagePath": "img"
    },
    {
      "path": "bundle.js",
      "addressable": true
    },
    {
      "path": "details.md",
      "addressable": true
    },
    {
      "path": "index.html",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "EdTro-AzureDevOps-Extensions-QueryBasedBoards.BoardTab",
      "type": "ms.vss-web.tab",
      "description": "Adds a 'Query Based Boards - Show Board / Show Taskboard' tab to the query results",
      "targets": [
        "ms.vss-work-web.query-tabs"
      ],
      "properties": {
        "uri": "index.html?type=board",
        "title": "Query Based Boards - Show Board / Show Taskboard",
        "name": "Board",
        "dynamic": true
      }
    },
    {
      "id": "EdTro-AzureDevOps-Extensions-QueryBasedBoards.ConfigTab",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'Query Based Boards - Configuration' hub to the project admin group",
      "targets": [
        "ms.vss-web.project-admin-hub-group"
      ],
      "properties": {
        "uri": "index.html?type=config",
        "title": "Query Based Boards - Configuration",
        "name": "Query Based Boards",
        "order": 100
      }
    }
  ],
  "links": {
    "privacypolicy": {
      "uri": "https://github.com/edtro/EdTro.AzureDevOps.Extensions.querybasedboards/blob/master/public/privacy.md"
    },
    "license": {
      "uri": "https://github.com/edtro/EdTro.AzureDevOps.Extensions.querybasedboards/blob/master/LICENSE"
    },
    "support": {
      "uri": "https://github.com/edtro/EdTro.AzureDevOps.Extensions.querybasedboards/issues"
    },
    "repository": {
      "uri": "https://github.com/edtro/EdTro.AzureDevOps.Extensions.querybasedboards"
    }
  }
}